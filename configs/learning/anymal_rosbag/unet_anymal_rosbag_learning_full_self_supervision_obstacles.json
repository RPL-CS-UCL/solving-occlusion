{
    "name": "unet_anymal_rosbag_learning_full_self_supervision_obstacles",
    "cuda": true,
    "experiment": {
        "task_path": {
            "defaults": {
                "loss": {
                    "report_frequency": 100,
                    "normalization": [],
                    "train_weights": {
                        "mse_rec_all": 0,
                        "mse_rec_nocc": 1,
                        "mse_rec_occ": 10,
                        "total_variation": 0.1
                    },
                    "eval_weights": {
                        "mse_rec_all": 0,
                        "mse_rec_nocc": 1,
                        "mse_rec_occ": 6
                    }
                },
                "domain_defaults": {
                    "batch_size": 6,
                    "datasets": [
                        {
                            "type": "hdf5",
                            "path": "data/2021-01-27-22-47-04_dataset_generation_anymal_rosbag_obstacles_maximilianstoelzle/DATASET_OCCLUSION.hdf5",
                            "size": [64, 64],
                            "sampling_weights": {"train":  1, "val":  1, "test":  1},
                            "self_supervision": true,
                            "transforms": {
                                "defaults": [
                                    {
                                        "type": "random_occlusion_dilation",
                                        "kernel_size": 3,
                                        "probability": 0.5
                                    },
                                    {
                                        "type": "random_occlusion",
                                        "probability": 0.03,
                                        "apply_to": ["occ_dem", "occ_mask"]
                                    }
                                ]
                            }
                        }
                    ],
                    "num_workers": 4,
                    "shuffle": true
                },
                "model": {
                    "name": "UNet",
                    "save_frequency": 1,
                    "input_dim": [64, 64],
                    "in_channels": ["occ_dem", "occ_mask"],
                    "out_channels": ["rec_dem"],
                    "hidden_dims": [64, 128, 256],
                    "input_normalization": false,
                    "NaN_replacement": 0,
                    "bilinear": true
                },
                "optimizer": {
                    "name": "Adam",
                    "lr": 0.0001,
                    "weight_decay": 0.001
                },
                "controller": {
                    "max_num_epochs": 50,
                    "max_num_better_results": 20
                },
                "dataloaders": {"labeled_dataloader":  "target"}
            },
            "tasks": [
                {
                    "task_type": "supervised-learning",
                    "model_to_train": "untrained",
                    "domains": {
                        "target": {}
                    }
                }
            ]
        },
        "visualization": {"sample_frequency":  25, "indiv_vranges": true}
    },
    "remote": true,
    "seeds": [101]
}
