{
    "name": "baseline_trasys_earning",
    "cuda": true,
    "experiment": {
        "task_path": {
            "defaults": {
                "loss": {
                    "report_frequency": 100,
                    "normalization": [],
                    "train_weights": {
                        "mse_rec_all": 1,
                        "mse_rec_occ": 0,
                        "mse_rec_nocc": 0
                    },
                    "eval_weights": {
                        "mse_rec_all": 0,
                        "mse_rec_occ": 1,
                        "mse_rec_nocc": 0
                    }
                },
                "domain_defaults": {
                    "batch_size": 5,
                    "dataset": {
                        "type": "trasys_planetary",
                        "path": "data/planetary-surface-dataset",
                        "size": [128, 128],
                        "transforms": {"train": {"random_horizontal_flip": {"probability": 0.5}}}
                    },
                    "num_workers": 1,
                    "shuffle": true
                },
                "model": {
                    "name": "Telea",
                    "save_frequency": 10,
                    "input_dim": [128, 128],
                    "in_channels": ["gt_dem"],
                    "out_channels": ["reconstructed_elevation_map"],
                    "input_normalization": false,
                    "inpaint_radius": 3
                },
                "optimizer": {
                    "name": "Adam",
                    "lr": 0.001,
                    "weight_decay": 0.001
                },
                "controller": {
                    "max_num_epochs": 0,
                    "max_num_better_results": 0
                }
            },
            "tasks": [
                {
                    "task_type": "supervised-learning",
                    "model_to_train": "untrained",
                    "domains": {"target": {}},
                    "dataloaders": {"labeled_dataloader":  "target"}
                }
            ]
        },
        "visualization": {"sample_frequency":  1}
    },
    "logging": {"upload": false},
    "remote": true,
    "seeds": [101]
}
