{
    "name": "unet_synthetic_height_map_learning_afd",
    "cuda": true,
    "experiment": {
        "task_path": {
            "defaults": {
                "loss": {
                    "report_frequency": 100,
                    "normalization": [],
                    "train_weights": {
                        "mse_rec_all": 0,
                        "mse_rec_nocc": 1,
                        "mse_rec_occ": 6,
                        "perceptual": 0.05,
                        "style": 120,
                        "total_variation": 0.1,
                        "kld": 1
                    },
                    "eval_weights": {
                        "mse_rec_all": 0,
                        "mse_rec_nocc": 0,
                        "mse_rec_occ": 1
                    }
                },
                "domain_defaults": {
                    "batch_size": 6,
                    "dataset": {
                        "type": "hdf5",
                        "path": "data/2020-10-02-15-09-00_dataset_generation_synthetic_height_map_dataset_generation_maximilianstoelzle/DATASET_OCCLUSION.hdf5",
                        "size": [64, 64]
                    },
                    "num_workers": 4,
                    "shuffle": true
                },
                "model": {
                    "name": "UNet",
                    "save_frequency": 1,
                    "input_dim": [64, 64],
                    "in_channels": ["occ_dem", "occ_mask"],
                    "out_channels": ["rec_dem"],
                    "hidden_dims": [64, 128, 256],
                    "input_normalization": false,
                    "NaN_replacement": 0,
                    "bilinear": true,
                    "feature_extractor": true,
                    "data_uncertainty_estimation": {
                        "method": "ADF",
                        "min_variance": 0.001
                    }
                },
                "optimizer": {
                    "name": "Adam",
                    "lr": 0.0001,
                    "weight_decay": 0.001
                },
                "controller": {
                    "max_num_epochs": 50,
                    "max_num_better_results": 20
                },
                "dataloaders": {"labeled_dataloader":  "target"}
            },
            "tasks": [
                {
                    "task_type": "supervised-learning",
                    "model_to_train": "untrained",
                    "domains": {"target": {}}
                }
            ]
        },
        "visualization": {"sample_frequency":  10}
    },
    "logging": {
        "upload": false
    },
    "remote": true,
    "seeds": [101]
}
