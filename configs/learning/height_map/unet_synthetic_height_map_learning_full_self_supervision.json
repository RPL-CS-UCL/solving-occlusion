{
    "name": "unet_synthetic_height_map_learning_full_self_supervision",
    "cuda": true,
    "experiment": {
        "task_path": {
            "defaults": {
                "loss": {
                    "report_frequency": 100,
                    "normalization": [],
                    "train_weights": {
                        "mse_rec_all": 0,
                        "mse_rec_nocc": 1,
                        "mse_rec_occ": 10,
                        "total_variation": 0.1
                    },
                    "eval_weights": {
                        "mse_rec_all": 0,
                        "mse_rec_nocc": 1,
                        "mse_rec_occ": 6
                    }
                },
                "domain_defaults": {
                    "batch_size": 6,
                    "datasets": [
                        {
                            "type": "hdf5",
                            "path": "data/2020-10-02-15-09-00_dataset_generation_synthetic_height_map_dataset_generation_maximilianstoelzle/DATASET_OCCLUSION.hdf5",
                            "size": [64, 64],
                            "sampling_weights": {"train": 1, "val":  1, "test":  0},
                            "self_supervision": true,
                            "transforms": {
                                "defaults": [
                                    {
                                        "type": "random_occlusion_dilation",
                                        "kernel_size": 3,
                                        "probability": 0.5
                                    },
                                    {
                                        "type": "random_occlusion",
                                        "probability": 0.03,
                                        "apply_to": ["occluded_elevation_map", "binary_occlusion_map"]
                                    }
                                ]
                            }
                        },
                        {
                            "type": "hdf5",
                            "path": "data/2020-10-02-15-09-00_dataset_generation_synthetic_height_map_dataset_generation_maximilianstoelzle/DATASET_OCCLUSION.hdf5",
                            "size": [64, 64],
                            "sampling_weights": {"train": 0, "val":  0, "test":  1}
                        }
                    ],
                    "num_workers": 4,
                    "shuffle": true
                },
                "model": {
                    "name": "UNet",
                    "save_frequency": 1,
                    "input_dim": [64, 64],
                    "in_channels": ["occluded_elevation_map", "binary_occlusion_map"],
                    "out_channels": ["reconstructed_elevation_map"],
                    "hidden_dims": [64, 128, 256],
                    "input_normalization": false,
                    "NaN_replacement": 0,
                    "bilinear": true,
                    "feature_extractor": true
                },
                "optimizer": {
                    "name": "Adam",
                    "lr": 0.0001,
                    "weight_decay": 0.001
                },
                "controller": {
                    "max_num_epochs": 50,
                    "max_num_better_results": 20
                },
                "dataloaders": {"labeled_dataloader":  "target"}
            },
            "tasks": [
                {
                    "task_type": "supervised-learning",
                    "model_to_train": "untrained",
                    "domains": {"target": {}}
                }
            ]
        },
        "visualization": {"sample_frequency":  10}
    },
    "logging": {
        "upload": false
    },
    "remote": true,
    "seeds": [101, 102, 103]
}
