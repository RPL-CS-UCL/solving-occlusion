{
    "name": "unet_tenerife_learning_syn2real_dilation_self_supervision_raycasting",
    "cuda": true,
    "experiment": {
        "task_path": {
            "defaults": {
                "loss": {
                    "report_frequency": 100,
                    "normalization": [],
                    "train_weights": {
                        "mse_rec_all": 0,
                        "mse_rec_nocc": 1,
                        "mse_rec_occ": 10,
                        "total_variation": 0.1
                    },
                    "eval_weights": {
                        "mse_rec_all": 0,
                        "mse_rec_nocc": 1,
                        "mse_rec_occ": 6
                    }
                },
                "domain_defaults": {
                    "batch_size": 6,
                    "num_workers": 4,
                    "shuffle": true
                },
                "model": {
                    "name": "UNet",
                    "save_frequency": 1,
                    "input_dim": [64, 64],
                    "in_channels": ["occ_dem", "occ_mask"],
                    "out_channels": ["rec_dem"],
                    "hidden_dims": [64, 128, 256],
                    "input_normalization": {"mean": true, "stdev": false},
                    "NaN_replacement": 0,
                    "bilinear": true
                },
                "optimizer": {
                    "name": "Adam",
                    "lr": 0.0001,
                    "weight_decay": 0.001
                },
                "controller": {
                    "max_num_epochs": 100,
                    "max_num_better_results": 35
                },
                "dataloaders": {"labeled_dataloader":  "target"}
            },
            "tasks": [
                {
                    "task_type": "supervised-learning",
                    "model_to_train": "untrained",
                    "domains": {
                        "target": {
                            "datasets": [
                                {
                                    "type": "hdf5",
                                    "path": "data/2021-02-17-22-25-54_dataset_generation_tenerife_pocolog_combined_user/DATASET_OCCLUSION.hdf5",
                                    "size": [64, 64],
                                    "sampling_weights": {"train":  1, "val":  1, "test":  1},
                                    "self_supervision": true,
                                    "transforms": {
                                        "defaults": [
                                            {
                                                "type": "random_occlusion_raycasting",
                                                "height_viewpoint": {"min":  0.3, "max":  1},
                                                "occluded_area_constraint": {
                                                    "min":  0.001,
                                                    "max": 0.6,
                                                    "max_iterations": 15,
                                                    "min_step": 0.15
                                                }
                                            }
                                        ],
                                        "test": [
                                            {
                                                "type": "random_occlusion",
                                                "probability": 0.025,
                                                "apply_to": ["occ_dem", "occ_mask"]
                                            },
                                            {
                                                "type": "random_occlusion_dilation",
                                                "kernel_size": 3,
                                                "probability": 0.5
                                            },
                                            {
                                                "type": "random_occlusion",
                                                "probability": 0.025,
                                                "apply_to": ["occ_dem", "occ_mask"]
                                            }
                                        ]
                                    }
                                }
                            ]
                        }
                    }
                },

            ]
        },
        "visualization": {"sample_frequency":  50, "indiv_vranges": true}
    },
    "remote": true,
    "seeds": [101]
}
