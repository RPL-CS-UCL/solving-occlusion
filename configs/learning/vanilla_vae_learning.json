{
    "name": "vanilla_vae_learning",
    "cuda": true,
    "experiment": {
        "task_path": {
            "defaults": {
                "loss": {
                    "report_frequency": 100,
                    "normalization": ["mse_rec_all", "mse_rec_occ"]
                },
                "domain_defaults": {
                    "batch_size": 32,
                    "dataset": {
                        "type": "hdf5",
                        "path": "data/2020-10-02-15-09-00_dataset_generation_synthetic_height_map_dataset_generation_maximilianstoelzle/DATASET_OCCLUSION.hdf5",
                        "size": [64, 64],
                        "transforms": {
                            "defaults": [{"type": "white_noise", "stdev": 0.1, "apply_to": ["occluded_elevation_map"]}]
                        }
                    },
                    "num_workers": 4,
                    "shuffle": true
                },
                "model": {
                    "name": "VanillaVAE",
                    "save_frequency": 10,
                    "input_dim": [64, 64],
                    "in_channels": ["occluded_elevation_map", "binary_occlusion_map"],
                    "out_channels": ["reconstructed_elevation_map"],
                    "latent_dim": 128,
                    "input_normalization": false,
                    "NaN_replacement": 0
                },
                "optimizer": {
                    "name": "Adam",
                    "lr": 0.001,
                    "weight_decay": 0.001
                },
                "controller": {
                    "max_num_epochs": 0,
                    "max_num_better_results": 0
                }
            },
            "tasks": [
                {
                    "task_type": "supervised-learning",
                    "model_to_train": "untrained",
                    "domains": {"target": {}},
                    "dataloaders": {"labeled_dataloader":  "target"}
                }
            ]
        },
        "visualization": {"sample_frequency":  10}
    },
    "logging": {"upload": false},
    "remote": true,
    "seeds": [101]
}
