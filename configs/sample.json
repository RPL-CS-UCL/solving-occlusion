{
    "name": "sample",
    "cuda": false,
    "dataset_generation": {
        "type": "synthetic",
        "terrain_types": ["HEIGHT_MAP"],
        "elevation_map": {
            "resolution": 0.04,
            "size": 64,
            "height_viewpoint": {"min":  0.3, "max":  0.8}
        },
        "num_train_samples": 100,
        "num_val_samples": 100,
        "num_test_samples": 100,
        "save_frequency": 10,
        "visualization": {
            "frequency": 50
        }
    },
    "experiment": {
        "task_path": {
            "defaults": {
                "loss": {
                    "loss_function": {
                        "domain_distance_metric": "fid",
                        "regularization_weights": [0.01, 0.00]
                    },
                    "loss_aggregator": {"name": "mean"},
                    "report_frequency": 100
                },
                "domain_defaults": {
                    "batch_size": 32,
                    "dataset": "CIFAR10",
                    "shuffle": true,
                    "num_workers": 4
                },
                "model": {
                    "name": "VanillaVAE",
                    "in_channels": 3,
                    "latent_dim": 128
                },
                "optimizer": {
                    "name": "SGD",
                    "lr": 0.001,
                    "weight_decay": 0.001,
                    "momentum": 0.9
                },
                "controller": {
                    "max_num_epochs": 1,
                    "max_num_better_results": 20
                }
            },
            "tasks": [
                {
                    "task_type": "supervised-learning",
                    "model_to_train": "untrained",
                    "domains": {
                        "target": {}
                    },
                    "dataloaders": {"labeled_dataloader":  "target"}
                }
            ]
        }
    },
    "logging": {
        "upload": false
    },
    "remote": false,
    "seeds": [101]
}
